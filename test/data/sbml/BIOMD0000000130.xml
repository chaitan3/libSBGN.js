  <listOfRules>
    <rateRule metaid="metaid_0000025" variable="v">
      <notes>
        <p xmlns="http://www.w3.org/1999/xhtml">For this model, the ODE for &quot;v&quot; is different from the others. v&apos;=0.04*v^2+4.1*v+108-u+i </p>
      </notes>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <plus/>
          <apply>
            <minus/>
            <apply>
              <plus/>
              <apply>
                <times/>
                <cn> 0.04 </cn>
                <apply>
                  <power/>
                  <ci> v </ci>
                  <cn type="integer"> 2 </cn>
                </apply>
              </apply>
              <apply>
                <times/>
                <cn> 4.1 </cn>
                <ci> v </ci>
              </apply>
              <cn type="integer"> 108 </cn>
            </apply>
            <ci> u </ci>
          </apply>
          <ci> i </ci>
        </apply>
      </math>
    </rateRule>
    <rateRule metaid="metaid_0000026" variable="u">
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <times/>
          <ci> a </ci>
          <apply>
            <minus/>
            <apply>
              <times/>
              <ci> b </ci>
              <ci> v </ci>
            </apply>
            <ci> u </ci>
          </apply>
        </apply>
      </math>
    </rateRule>
  </listOfRules>
