  <listOfRules>
    <assignmentRule metaid="metaid_0000023" variable="n">
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <times/>
          <ci> n0 </ci>
          <apply>
            <plus/>
            <apply>
              <exp/>
              <apply>
                <times/>
                <apply>
                  <minus/>
                  <ci> kbN </ci>
                </apply>
                <csymbol encoding="text" definitionURL="http://www.sbml.org/sbml/symbols/time"> Time </csymbol>
              </apply>
            </apply>
            <apply>
              <times/>
              <ci> kappa </ci>
              <apply>
                <minus/>
                <cn type="integer"> 1 </cn>
                <apply>
                  <exp/>
                  <apply>
                    <times/>
                    <apply>
                      <minus/>
                      <ci> kbN </ci>
                    </apply>
                    <csymbol encoding="text" definitionURL="http://www.sbml.org/sbml/symbols/time"> Time </csymbol>
                  </apply>
                </apply>
              </apply>
            </apply>
          </apply>
        </apply>
      </math>
    </assignmentRule>
    <assignmentRule metaid="metaid_0000024" variable="p1">
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <plus/>
          <ci> p11 </ci>
          <apply>
            <divide/>
            <apply>
              <times/>
              <ci> p12 </ci>
              <ci> n </ci>
            </apply>
            <apply>
              <plus/>
              <ci> K1 </ci>
              <ci> n </ci>
            </apply>
          </apply>
        </apply>
      </math>
    </assignmentRule>
    <assignmentRule metaid="metaid_0000025" variable="p2">
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <divide/>
          <ci> Ca_Cyt </ci>
          <apply>
            <plus/>
            <ci> K2 </ci>
            <ci> Ca_Cyt </ci>
          </apply>
        </apply>
      </math>
    </assignmentRule>
    <rateRule metaid="metaid_0000026" variable="p3">
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <plus/>
          <apply>
            <minus/>
            <apply>
              <times/>
              <ci> k31 </ci>
              <ci> Ca_Cyt </ci>
              <ci> p3 </ci>
            </apply>
          </apply>
          <apply>
            <times/>
            <ci> k31 </ci>
            <ci> K3 </ci>
            <apply>
              <minus/>
              <cn type="integer"> 1 </cn>
              <ci> p3 </ci>
            </apply>
          </apply>
        </apply>
      </math>
    </rateRule>
  </listOfRules>
